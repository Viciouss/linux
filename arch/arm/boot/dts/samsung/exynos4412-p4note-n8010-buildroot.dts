/dts-v1/;
#include "exynos4412-p4note.dtsi"

/ {
	model = "Samsung Galaxy Note 10.1 (GT-N8010/N8013) based on Exynos4412";
	compatible = "samsung,n8010", "samsung,p4note", "samsung,exynos4412", "samsung,exynos4";
	chassis-type = "tablet";

	chosen {
		bootargs = "root=/dev/mmcblk0p9 rw init=/linuxrc earlyprintk console=ttySAC2 console=tty0 androidboot.selinux=permissive androidboot.hardware=smdk4x12";
		stdout-path = "serial2:115200n8";
	};
};